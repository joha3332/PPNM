set term svg
set decimalsign locale
set out "PlotA.svg"
set encoding utf8
set xlabel 'x'
set ylabel "y"
set key top right
set xzeroaxis
set yzeroaxis
set title "Rutherford and Soddy experiment"


plot "data.txt" index 0 using ($1):($2):($3) with errorbars title "Experimental data"\
	,"data.txt" index 1 using ($1):($2) with lines title "Exponetial fit"\

	

