set term svg
set decimalsign locale
set out "PlotB.svg"
set encoding utf8
set xlabel 'x'
set ylabel "y"
set key top right
set xzeroaxis
set yzeroaxis
set title "Qspline of sin(x)"
 

plot [0:10][-1.2:1.4] \
	"out_qspline.txt" using ($1):($2) with lines lc rgb "blue" title "Quardratic interpolation"\
	,"input.txt" using ($1):($2) ls 7 lc rgb "red" title "Analytic values of sin(x)"\
	

