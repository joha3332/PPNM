set term svg
set decimalsign locale
set out "PlotC_integral.svg"
set encoding utf8
set xlabel 'x'
set ylabel "y"
set key top right
set xzeroaxis
set yzeroaxis
set title "Cspline - integral of sin(x)"
 

plot [0:10][-0.5:2.5] \
	"out_cspline_integral.txt" using ($1):($2) with lines lc rgb "blue" title "Integral of cubic interpolation"\
	, 1-cos(x) dashtype 2 lw 3 lc rgb "red" title "Analytic value of integral"\
	

