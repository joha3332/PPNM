set term svg
set decimalsign locale
set out "PlotC_slope.svg"
set encoding utf8
set xlabel 'x'
set ylabel "y"
set key top right
set xzeroaxis
set yzeroaxis
set title "Cspline - slope of sin(x)"
 

plot [0:10][-1.2:1.4] \
	"out_cspline_slope.txt" using ($1):($2) with lines lc rgb "blue" title "Slope of cubic interpolation"\
	, cos(x) dashtype 2 lw 3 lc rgb "red" title "Analytic value of slope"\
	

