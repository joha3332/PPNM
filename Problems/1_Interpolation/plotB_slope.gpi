set term svg
set decimalsign locale
set out "PlotB_slope.svg"
set encoding utf8
set xlabel 'x'
set ylabel "y"
set key top right
set xzeroaxis
set yzeroaxis
set title "Qspline - slope of sin(x)"
 

plot [0:10][:] \
	"out_qspline_slope.txt" using ($1):($2) with lines lc rgb "blue" title "Slope of quardratic interpolation"\
	, cos(x) dashtype 2 lw 3 lc rgb "red" title "Analytic value of slope"\
	

